{
	"compilerOptions": {
		"target": "es2020",
		"lib": [
			"es2020",
			"WebWorker",
			"DOM"
		],
		"module": "ESNext",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"outDir": "out",
		"rootDir": "src",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"declaration": true,
		"composite": true,
	},
	"include": [
		"src",
	],
	"exclude": [
		"node_modules"
	]
}